{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-key": "error",
    "react/jsx-no-useless-fragment": "warn",
    "react-hooks/exhaustive-deps": "warn"
  }
}